generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model facturas_compras {
  id        String   @id @default(cuid())
  timbrado  Int
  ruc       String
  nombre    String
  condicion String
  valor     Int
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}

model facturas_ventas {
  id        String   @id @default(cuid())
  timbrado  Int
  ruc       String
  nombre    String
  condicion String
  valor     Int
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}

model contadores {
  id        String   @id(map: "contador_pkey")
  ruc       String
  createdAt DateTime @default(now())
  updatedAt DateTime
  nombre    String
}

model vendedores {
  id        String   @id(map: "vendedor_pkey")
  ruc       String
  createdAt DateTime @default(now())
  updatedAt DateTime
  nombre    String
}
